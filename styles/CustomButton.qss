MenuButton
{
    background-color: yellow;
    border-width: 5px;
    border-color: black;
    border-radius: 25px;
    border-style:inset;
    font-size: 30px;
}
MenuButton:hover
{
    background-color: green;
}
MenuButton:pressed
{
    background-color: red;
}

QPushButton#BackImageButton
{
    border-image: url(:/images/back.png);
    border-radius: 10px;
}
QPushButton#BackImageButton:hover
{
    background-color: white;
}
QPushButton#BackImageButton:pressed
{
    background-color: black;
}
